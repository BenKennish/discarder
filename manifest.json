{
  "manifest_version": 3,
  "name": "Discarder",
  "description": "Allows on-demand discarding of all tabs, optionally respecting auto discardable",
  "version": "0.2",
  "icons": 
  {
    //"16": "icon16.png",
    //"48": "icon48.png",
    "128": "icon128.png"
  },
  "action": 
  {
    "default_popup": "popup.html",
    "default_icon": 
    {
      //"48": "/assets/icons/48.png",
      "128": "icon128.png"
    }
  },
  "permissions": 
  [
    "tabs",
    "notifications",
    "system.memory"
  ]
  /* "background": 
  {
    "service_worker": "background.js"
  } */
}